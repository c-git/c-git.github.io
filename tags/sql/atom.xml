        <link rel="alternate" type="text/html" href="https://dev.chester.wykies.com/sql/misc/"/>
        <id>https://dev.chester.wykies.com/sql/misc/</id>
        
        <content type="html" xml:base="https://dev.chester.wykies.com/sql/misc/">&lt;p&gt;Source: &lt;a rel=&quot;noopener&quot; target=&quot;_blank&quot; href=&quot;https:&#x2F;&#x2F;www.digitalocean.com&#x2F;community&#x2F;tutorials&#x2F;how-to-allow-remote-access-to-mysql&quot;&gt;https:&#x2F;&#x2F;www.digitalocean.com&#x2F;community&#x2F;tutorials&#x2F;how-to-allow-remote-access-to-mysql&lt;&#x2F;a&gt;&lt;&#x2F;p&gt;
&lt;h1 id=&quot;open-command-prompt&quot;&gt;&lt;a class=&quot;zola-anchor&quot; href=&quot;#open-command-prompt&quot; aria-label=&quot;Anchor link for: open-command-prompt&quot;&gt;Open Command Prompt&lt;&#x2F;a&gt;&lt;&#x2F;h1&gt;
